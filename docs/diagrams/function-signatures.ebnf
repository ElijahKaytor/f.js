signature ::= (name '=')? type ';'
name ::= [a-z] (('.')? [a-zA-Z0-9_])*
type ::= (
    'Fn' ('(' (name ':')? type (',' (name ':')? type)* (',')? ')')? ('->' type)? ('=' (type | ECMAScriptExpression))?
    | 'Generic'
    | 'List' ('(' type ')')?
    | 'Object'
    | 'Int'
    | 'Float'
    | 'String'
    | [A-Z] [A-Za-z0-9]*
)

